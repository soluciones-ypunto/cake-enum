{
    "name": "soluciones-ypunto/cake-enum",
    "description": "Librería básica para implementar enums en CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-3.0-or-later",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ypunto\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ypunto\\Enum\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
